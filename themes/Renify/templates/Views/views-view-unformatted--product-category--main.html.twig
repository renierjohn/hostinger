{{ attach_library('core/drupal') }}
<div class="flex-1-3">
    <div class="p-sidebar">
      <div class="p-row">
        <div class="p-sidebar-search">
          <input placeholder="Search" name="search" value="" size="300" maxlength="254">
        </div>
      </div>
      <div class="p-row">
        <div class="p-sidebar-menu">
          <ul>
            <li>
              <h5>Menu</h5>
            </li>
            <li>
              <a href="/products/" class="p-sidebar-menu-all" title="Products">
                <div class="p-list">
                  <div class="p-img-wrapper">
                    <img src="/sites/default/files/public/products/main.jpg">
                  </div>
                  <div class="p-txt-wrapper">
                    All
                  </div>
                </div>
              </a>
            </li>
            {% if rows %}
              {% for row in rows %}
                {{row.content}}
              {% endfor %}
            {% endif %}
          </ul>
        </div>
      </div>
      <div class="p-row">
        <div class="p-sidebar-category">
        </div>
      </div>
      <div class="p-row">
        <div class="p-sidebar-menu">
        </div>
      </div>
    </div>
  </div>