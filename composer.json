{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/admin_toolbar": "^3.2",
        "drupal/aggregator": "^2.0",
        "drupal/captcha": "^1.9",
        "drupal/color": "^1.0",
        "drupal/config_ignore": "^2.4",
        "drupal/core-composer-scaffold": "9.5.9",
        "drupal/core-project-message": "9.5.9",
        "drupal/core-recommended": "9.5.9",
        "drupal/core-vendor-hardening": "^9.5.9",
        "drupal/ctools": "^4.0",
        "drupal/devel": "5.1.1",
        "drupal/google_analytics": "^4.0",
        "drupal/google_tag": "^1.6",
        "drupal/image_class": "^2.0",
        "drupal/imce": "^2.5",
        "drupal/link_class": "^2.1",
        "drupal/metatag": "^1.22",
        "drupal/paragraphs": "^1.15",
        "drupal/pathauto": "^1.11",
        "drupal/quickedit": "^1.0",
        "drupal/rdf": "^2.1",
        "drupal/redirect": "^1.8",
        "drupal/rest_api_access_token": "^1.6",
        "drupal/schema_metatag": "^2.4",
        "drupal/stage_file_proxy": "^2.0",
        "drupal/twig_tweak": "^3.2",
        "drupal/xmlsitemap": "^1.4",
        "drush/drush": "^11.5",
        "kint-php/kint": "^5.0",
        "phpmailer/phpmailer": "^6.6",
        "symfony/dotenv": "^6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": false,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-vendor-hardening": true
        },
        "sort-packages": true
    },
    "extra": {
        "installer-paths": {
            "core": ["type:drupal-core"],
            "modules/contrib/{$name}" : ["type:drupal-module"],
            "modules/custom/{$name}" : ["type:drupal-custom-module"],
            "themes/contrib/{$name}" : ["type:drupal-theme"],
            "themes/custom/{$name}" : ["type:drupal-custom-theme"],
            "libraries/{$name}" : ["type:drupal-library"],
            "profiles/contrib/{$name}" : ["type:drupal-profile"],
            "profiles/custom/{$name}" : ["type:drupal-custom-profile"],
            "drush/{$name}" : ["type:drupal-drush"],
            "config/{name}" : ["type:drupal-config"]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "drupal-core-vendor-hardening": {
            "vendor/package": ["test", "documentation"]
        }
    }
}
